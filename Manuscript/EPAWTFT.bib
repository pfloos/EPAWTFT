	Shorttitle = {Holomorphic {{Hartree}}\textendash{{Fock Theory}}},
	Title = {Holomorphic {{Hartree}}\textendash{{Fock Theory}}: {{The Nature}} of {{Two}}-{{Electron Problems}}},
	Volume = {14},
