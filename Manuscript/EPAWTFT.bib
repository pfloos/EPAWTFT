	Issn = {0305-4470, 1361-6447},
	Journal = {J. Phys. Math. Gen.},
	Month = oct,
	Number = {43},
	Pages = {10139-10165},
	Title = {The {{\cal C}} Operator in {{\cal PT}}-Symmetric Quantum Theories},
