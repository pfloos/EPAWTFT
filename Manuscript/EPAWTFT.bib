	File = {/Users/loos/Zotero/storage/IL7CHRFF/Filatov - 2015 - Ensemble DFT Approach to Excited States of Strongl.pdf},
	Isbn = {978-3-319-22080-2 978-3-319-22081-9},
	Pages = {97-124},
